  "devDependencies": {
    "@absolunet/fsp": "^1.6.1",
    "acorn": "^6.1.1",
    "babel-jest": "^24.8.0",
    "babel-preset-jaid": "^6.8.0",
    "clean-webpack-plugin": "^3.0.0",
    "eslint": "^6.0.1",
    "eslint-config-jaid": "^1.18.0",
    "jest": "^24.8.0",
